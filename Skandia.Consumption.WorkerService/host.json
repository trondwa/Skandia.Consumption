{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "queues": {
      "batchSize": 12,
      "newBatchThreshold": 6,
      "maxPollingInterval": "00:00:02",
      "visibilityTimeout": "00:10:00",
      "messageEncoding": "base64"
    }
  }
}